buildscript {
    repositories {
        maven { url "https://maven.aliyun.com/repository/jcenter" }  // <= here
        maven { url "https://maven.aliyun.com/repository/google" }  // <= here
        maven { url "https://maven.aliyun.com/nexus/content/repositories/central" }
        maven { url "https://maven.aliyun.com/repository/public/" }
        jcenter() // 保留
        google() // 保留
    }
    dependencies {
        classpath 'io.spring.javaformat:spring-javaformat-gradle-plugin:0.0.25'
        classpath 'com.google.protobuf:protobuf-gradle-plugin:0.8.13'
//        classpath 'net.saliman:gradle-properties-plugin:1.5.1'
    }
}


allprojects {
    repositories {
        maven { url "https://maven.aliyun.com/repository/jcenter" }  // <= here
        maven { url "https://maven.aliyun.com/repository/google" }  // <= here
        maven { url "https://maven.aliyun.com/nexus/content/repositories/central" }
        maven { url "https://maven.aliyun.com/repository/public/" }
        jcenter() // 保留
        google() // 保留
    }
    apply plugin: 'idea'
    group = 'com.aihangxunxi.aitalk'
    version = '2.0-SNAPSHOT'
}

subprojects {
    apply plugin: 'java'
    apply plugin: 'io.spring.javaformat'

    java {
        sourceCompatibility = JavaVersion.VERSION_1_8
        targetCompatibility = JavaVersion.VERSION_1_8
    }

}






